<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.femass.controller.MainController">
   <center>
      <HBox prefHeight="79.0" prefWidth="695.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
              <Button fx:id="BtnCategories" onAction="#BtnCategories_Action" prefHeight="24.0" prefWidth="129.0" text="Categorias">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Roboto Mono Bold" size="13.0" />
               </font></Button>
            <Button fx:id="BtnSuppliers" onAction="#BtnSuppliers_Action" prefHeight="24.0" prefWidth="129.0" text="Fornecedores">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Roboto Mono Bold" size="13.0" />
               </font></Button>
            <Button fx:id="BtnClients" onAction="#BtnClients_Action" prefHeight="24.0" prefWidth="129.0" text="Clientes">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Roboto Mono Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="BtnProducts" onAction="#BtnProducts_Action" prefHeight="24.0" prefWidth="129.0" text="Produtos">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Roboto Mono Bold" size="13.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </center>
</BorderPane>
