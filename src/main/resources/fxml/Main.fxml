<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.femass.controller.MainController">
   <center>
      <HBox prefHeight="220.0" prefWidth="250.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="270.0" prefWidth="215.0" spacing="15.0">
               <children>
                    <Button fx:id="BtnCategories" onAction="#BtnCategories_Action" prefHeight="24.0" prefWidth="129.0" text="Categorias">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button fx:id="BtnSuppliers" onAction="#BtnSuppliers_Action" prefHeight="24.0" prefWidth="129.0" text="Fornecedores">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button fx:id="BtnClients" onAction="#BtnClients_Action" prefHeight="24.0" prefWidth="129.0" text="Clientes">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button fx:id="BtnProducts" onAction="#BtnProducts_Action" prefHeight="24.0" prefWidth="129.0" text="Produtos">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button fx:id="BtnPurchases" onAction="#BtnPurchases_Action" prefHeight="24.0" prefWidth="129.0" text="Compras">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </center>
</BorderPane>
